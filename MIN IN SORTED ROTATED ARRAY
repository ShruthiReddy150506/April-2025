def MinimumRoated(nums):
    n=len(nums)
    l=0
    r=n-1
    if nums[0]<nums[-1]:
        return nums[0]
    while l<=r:
        mid=(l+r)//2
        if nums[mid]>nums[mid+1]:
            return nums[mid+1]
        elif nums[mid-1]>nums[mid]:
            return nums[mid]
        elif nums[0]>nums[mid]:
            r=mid-1
        else:
            l=mid+1
nums=list(map(int,input("enter:").split()))
print(MinimumRoated(nums))
